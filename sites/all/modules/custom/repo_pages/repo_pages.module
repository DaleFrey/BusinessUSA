function repo_pages_init() {
    
    drupal_register_shutdown_function(
        function () {
            
            if ( !empty($GLOBALS['drupal_output_buffering_start_time']) ) {

                $status = drupal_get_http_header("status");
                if ( $status == "404 Not Found" ) {
            
                    $uriWithoutQueryString = strtok($_SERVER['REQUEST_URI'], '?');
                    $testTarget = "sites/all/pages/{$uriWithoutQueryString}.page.php";
                    if ( file_exists($testTarget) ) {
                    
                        @ob_end_clean();
                        while (@ob_end_clean());
                        print "
                            <h1>Inconsistency in Drupal menu-cache detected!</h1>
                            For some reason Drupal did not pick up the fact that the file {$testTarget} exists...<br/> 
                            This can happen when you don't flush Drupal menu-cache after creating a new file in sites/all/pages, doing an SVN update, 
                            or if the menu-cache is invalidated or corrupted.<br/>
                            <br/>
                            <div style=\"border: 1px solid gray; padding: 15px; font-weight: bold;\">
                                <img style=\"width: 25px; padding-right: 15px;\" src=\"data:image/gif;base64,\" />
                                Please wait, flushing Drupal cache... 
                            </div>
                            <br/>
                            Once this [cache-flush] process is complete, this page will automatically reload. If this process takes more then 5 minutes, please manually refresh this page.<br/>
                            <br/>
                            <i>This message has been triggered and rendered from repo_pages.module Coder Bookmark: CB-U318HUV-BC/</i>
                        ";
                        
                        error_log('Clearing all Drupal caches as per repo_pages.module Coder Bookmark: CB-PKHMG6N-BC');
                        drupal_flush_all_caches();
                        
                        print '<script>document.location.reload();</script>';
                    }
                }
            } else {
                error_log(
                    'WARNING: You do not appear to have the drupal_output_buffering module enabeled.' . 
                    'Without this the repo_pages module cannot notify the use when there is a false-positive on a 404 error (due to Drupal-menu-caching issues)'
                );
            }
        }
    );
    
}

/**
 * Implements hook_menu().
 */
function repo_pages_menu() {

    $items = array();
    
    $items['repo_pages_module/js-read'] = array(
        'title' => 'Hidden JavaScript Access Workaround (RepoPages Module)',
        'description' => 'JavaScript filenames that has a dot/period as the first character may not be accessible througn Apache, this menu hook acts as a workaround for this.',
        'page callback' => 'repo_pages_javascript_serv_workaround',
        'access callback' => true,
        'type' => MENU_CALLBACK,
    );
    
    $containerDirectory = 'sites/all/pages/';
    $repoPages = _repo_pages_find_pages_files($containerDirectory);
    foreach ( $repoPages as $urlPath => $pageInfo ) {

        $urlPath = str_replace($containerDirectory, '', $urlPath);
        error_log("repo_pages module is registering the Drupal-menu path of $urlPath with the title of \"{$pageInfo['title']}\" to execute the script at {$pageInfo['path']} ");
        $items[$urlPath] = array(
            'title' => $pageInfo['title'],
            'description' => 'Invokes file: ' . $pageInfo['path'],
            'page callback' => 'repo_pages_fetch_page',
            'page arguments' => array(
                $pageInfo['path'],
                module_exists('drupal_output_buffering')
            ),
            'access callback' => true,
            'type' => MENU_CALLBACK,
        );
    
    }
    
    return $items;
}

/** mixed repo_pages_fetch_page(string $filePath)
  *
  * If the $filePath does not exist, RETURNS FALSE
  * If the $filePath exists, the $filePath is run through include() ...
  *   if the include() function returns anything other than the integer 1, the returned value from include() is returned by this function.
  *   if the include() function returns the integer 1 (which is default for this function), all print statements called from executing this $filePath-file is returned (HTML is returned)
  */
function repo_pages_fetch_page($filePath = '', $allowSoloMode = false) {
    
    if ( !file_exists($filePath) ) {
        return false;
    }
    
    $pageInfo = _repo_pages_get_page_info($filePath);
    
    // If this is a full-path, translate this to a relative path
    if ( substr($filePath, 0, 1) === '/' || substr($filePath, 1, 2) === ":\\" ) {
        $filePath = realpath($filePath);
        $filePath = str_replace(realpath(DRUPAL_ROOT), '', $filePath);
        $filePath = str_replace("\\", '/', $filePath);
        $filePath = ltrim($filePath, '/');
        @header("RepoPagePath: $filePath");
    }
    
    @header( "RepoPageFinalPath: " . overridable($filePath) );
    
    $html = '';

    // Debug and bug killers
    if ( $pageInfo['render mode'] != 'solo' || intval($allowSoloMode) !== 1 ) {
    
        $printr = print_r($pageInfo, true);
        $html .= "
            <!--
                DEBUG INFO: repo_pages_fetch_page
                    allowSoloMode = {$allowSoloMode}
                    _repo_pages_get_page_info( filePath ) = {$printr}
            -->
        ";
        
        $html .= "
            <!-- Bug Killer - The title of pages should be pulled from the ~.page.info file. /* Coder Bookmark: CB-YO4XWCR-BC */ -->
            <script rendersource=\"" . basename(__FILE__) . "\">
                jQuery(document).ready( function () {
                    /* Bug Killer - The title of pages should be pulled from the ~.page.info file. Coder Bookmark: CB-2C7SSZB-BC */
                    jQuery('#titleWrapper h1').text('{$pageInfo['title']}');
                    jQuery('#titleWrapper h1').attr('note', 'altered by jQuery at Coder Bookmark: CB-2C7SSZB-BC');
                });
            </script>
        ";
    }
    
    // Autodetect and include TARGET.page.inc file
    $incPath = str_replace('.page.php', '.page.inc', $filePath);
    $incPath = overridable($incPath);
    if ( file_exists($incPath) ) {
        ob_start();
        include($incPath);
        ob_end_clean(); // We do not allow print statements in this .inc convention
    }
    
    // Autodetect and add TARGET.page.js file
    $jsPath = str_replace('.page.php', '.page.js', $filePath);
    $jsPath = overridable($jsPath);
    if ( file_exists($jsPath) ) {
        $publicPath = str_replace(realpath(DRUPAL_ROOT), '', realpath($jsPath));
        $publicPath = "/" . ltrim(str_replace("\\", "/", $publicPath), "/");
        // Filenames that has a dot as the first character may not be accessible througn Apache, use this workaround
        if ( substr(basename($jsPath), 0, 1) === '.' ) {
            $publicPath = '/repo_pages_module/js-read?path=' . ltrim($publicPath, '/');
        }
        drupal_add_js(
            $publicPath, 
            array(
                'type' => 'file',
                'group' => JS_THEME,
            )
        );
    }
    
    // Autodetect and add TARGET.page.less file
    $lessPath = str_replace('.page.php', '.page.less', $filePath);
    if ( file_exists($lessPath) ) {
        @header("RepoLessPath: " . $lessPath);
        drupal_add_css(
            $lessPath, 
            array(
                'type' => 'file',
                'group' => CSS_THEME,
            )
        );
    }
    
    ob_start();
    $incReturn = include(overridable($filePath));
    $html .= ob_get_contents();
    ob_end_clean();
    
    // Allow $_GET to override $pageInfo['render mode']
    if ( !empty($_GET['render_mode']) ) {
        $pageInfo['render mode'] = $_GET['render_mode'];
    }
    
    // "solo" rendering mode - Drupal's rendering engine with no be used. ONLY the HTML/[PHP] in this file will be returned to the client-browser. Note, this requieres the usage of the drupal_output_buffering module.
    if ( $pageInfo['render mode'] == 'solo' && intval($allowSoloMode) === 1 ) {
        @ob_end_clean();
        while (@ob_end_clean());
        print $html;
        exit();
    }
    
    // "body" rendering mode - The HTML/[PHP] in this file will be the ONLY markup between the <body> and </body> tag. ALL other markup within the <body> tag is destroyed. Note that this option preserves the <head> tag.
    if ( $pageInfo['render mode'] == 'body' ) {
        $GLOBALS['overrideBodyMarkup'] = $html;
    }

    if ( $incReturn !== 1 ) {
        return $incReturn;
    } else {
        return $html;
    }
}

function _repo_pages_find_pages_files($dir = 'sites/all/pages/', $prefix = '.page.php') {
    
    //error_log("_repo_pages_find_pages_files() has been called with dir={$dir} prefix={$prefix}");
    //error_log($dir);
    
    global $repoPagesScannedDir;
    if ( !isset($repoPagesScannedDir) ){
        $repoPagesScannedDir = array();
    } else {
        if ( in_array(realpath($dir), $repoPagesScannedDir) ) {
            return array();
        } else {
            $repoPagesScannedDir[] = realpath($dir);
        }
    }
    
    $drupalRoot = realpath('./');
    $dir = realpath($dir) . '/';
    $result = array();
    
    if ( is_file($dir . '.page.php') ) {
        $f = $dir . '.page.php';
        $uri = str_replace(array($drupalRoot . '/', $drupalRoot . "\\"), '', $f);
        $uri = str_replace("\\", '/', $uri);
        $uri = str_replace('.page.php', '', $uri);
        $uri = rtrim($uri, '/');
        $result[$uri] = _repo_pages_get_page_info($f);
    }
    
    if ( function_exists('overridable_glob') ) {
        $files = overridable_glob($dir . '*', GLOB_MARK);
    } else {
        $files = glob($dir . '*', GLOB_MARK);
    }
    
    foreach ( $files as $f ) {
        if ( substr($f, -1) === '/' || substr($f, -1) === "\\" ) {
        
            // Bug killer - restrict to DRUPAL_ROOT and sites/default/files
            $realpathDir = realpath($f);
            $realpathDirOver = realpath('sites/default/files/emover/' . $f);
            $realpathDrupalRoot = realpath(DRUPAL_ROOT . '/sites/all/pages');
            $realpathFilesDir = realpath('sites/default/files/emover');
            if ( 
                strpos($realpathDir, $realpathDrupalRoot) !== 0 
                && strpos($realpathDirOver, $realpathDrupalRoot) !== 0 
                && strpos($realpathDir, $realpathFilesDir) !== 0 
                && strpos($realpathDirOver, $realpathFilesDir) !== 0 
            ) {
                //error_log(__FUNCTION__ . " is exiting, {$dir} (rp={$realpathDir},{$realpathDirOver}) is not inside of {$realpathDrupalRoot} nor {$realpathFilesDir} ");
                //return array();
            } else {
                $result = array_merge($result, _repo_pages_find_pages_files($f, $prefix));
            }
        } else {
            if ( substr($f, -strlen($prefix)) == $prefix ) { // this if block checks and verifies the file-extension
                $f = $dir . basename($f);
                $uri = str_replace(array($drupalRoot . '/', $drupalRoot . "\\"), '', $f);
                $uri = str_replace("\\", '/', $uri);
                $uri = str_replace('.page.php', '', $uri);
                $uri = rtrim($uri, '/');
                $result[$uri] = _repo_pages_get_page_info($f);
            }
        }
    }

    return $result;
}

function _repo_pages_get_page_info($pageFilePath) {

    // Default values
    $defaultTitle = strtok($_SERVER['REQUEST_URI'], '?');
    $defaultTitle = array_pop(explode('/', $defaultTitle));
    $defaultTitle = str_replace('-', ' ', $defaultTitle);
    $defaultTitle = ucwords(strtolower($defaultTitle));
    $info = array(
        'title' => $defaultTitle,
        'path' => $pageFilePath,
        'index in solr' => '',
        'inject dev note' => '1',
        'render mode' => 'normal',
    );
    $infoFilePath = str_replace('.page.php', '.page.info', $pageFilePath);
    $infoFilePath = overridable($infoFilePath);
    if ( file_exists($infoFilePath) && substr($infoFilePath, -10) === '.page.info' ) {
        @header( "RepoPageInfoFinalPath: " . $infoFilePath );
        $iniData = parse_ini_file($infoFilePath);
        if ( !is_array($iniData) ) {
            error_log("Error in _repo_pages_get_page_info() - {$infoFilePath} does not seem to be a valid ini file");
        } else {
            $info = array_merge($info, $iniData);
        }
    }
    
    // Enforce strict type-casting
    if ( intval($info['index in solr']) === 1 || strtolower($info['index in solr']) === 'true' ) {
        $info['index in solr'] = 1;
    }
    if ( intval($info['inject dev note']) === 1 || strtolower($info['inject dev note']) === 'true' ) {
        $info['inject dev note'] = 1;
    }
    return $info;

}

function repo_pages_javascript_serv_workaround() {
    
    if ( empty($_GET['path']) ) {
        return 'Missing path parameter';
    }
    
    if ( substr(basename($_GET['path']), 0, 1) !== '.' ) {
        header('HTTP/1.0 403 Forbidden');
        return 'Rejected - This filename does not begin with a dot';
    }
    
    if ( substr(basename($_GET['path']), -3) !== '.js' ) {
        header('HTTP/1.0 403 Forbidden');
        return 'Rejected - This filename does not end with .js';
    }
    
    if ( !file_exists($_GET['path']) ) {
        header("HTTP/1.0 404 Not Found");
        return 'JavaScript File Not Found';
    }
    
    $realFilePath = realpath($_GET['path']);
    $realDrupalRoot = realpath(DRUPAL_ROOT);
    if ( substr($realFilePath, 0, strlen($realDrupalRoot)) !== $realDrupalRoot ) {
        header('HTTP/1.0 403 Forbidden');
        return 'Rejected - The target file is not within the WebRoot';
    }
    
    @ob_end_clean();
    while (@ob_end_clean());
    header('Content-Type: application/javascript');
    readfile($_GET['path']);
    exit();
}









